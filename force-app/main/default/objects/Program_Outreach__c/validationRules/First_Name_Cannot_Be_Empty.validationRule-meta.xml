<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>First_Name_Cannot_Be_Empty</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISNULL(First_Name__c),ISBLANK(First_Name__c))</errorConditionFormula>
    <errorDisplayField>First_Name__c</errorDisplayField>
    <errorMessage>First Name Cannot Be Blank</errorMessage>
</ValidationRule>
