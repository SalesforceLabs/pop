<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_Cannot_Be_Blank</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISNULL(Phone__c),ISBLANK(Phone__c))</errorConditionFormula>
    <errorDisplayField>Phone__c</errorDisplayField>
    <errorMessage>Phone Cannot Be Blank</errorMessage>
</ValidationRule>
